
#include <stdio.h>
#include <stdlib.h>

using namespace std;

void count(int* arr, int size)
{
    int i;
    int sum=0;
    for (i = 0; i < size; ++i)
    	++sum;
        printf("%d ", sum);
}

int main() {
	//input how many eggs
	int arr[]={1, 2, 3, 4, 5, 6, 7};
	//count eggs
	int size = sizeof(arr)/sizeof(arr[0]);
	count(arr, size);
}
